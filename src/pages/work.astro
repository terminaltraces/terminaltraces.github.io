    <!-- Hero Section -->
    <section class="py-20 bg-gradient-hero">
        <div class="container mx-auto px-6">
            <div class="max-w-4xl mx-auto text-center space-y-8">
                <h1 class="text-5xl md:text-6xl font-bold text-gradient-cosmic">
                    Professional Journey
                </h1>
                <p
                    class="text-xl md:text-2xl text-cosmic-600 dark:text-cosmic-400 max-w-3xl mx-auto"
                >
                    Building products, leading teams, and solving complex
                    problems with code and coffee ☕
                </p>
            </div>
        </div>
    </section>

    <!-- Timeline Section -->
    <section class="py-20 bg-white dark:bg-cosmic-900">
